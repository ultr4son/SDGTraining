@using PeopleProTraining.Models
@model IEnumerable<TableDisplayModel>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ItemName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            @Html.Partial("../Shared/DisplayTemplates/TableRowDisplay", new TableRowDisplayModel { DisplayValue = item.ItemName, EditId = item.ItemId })
        </tr>
    }

</table>


